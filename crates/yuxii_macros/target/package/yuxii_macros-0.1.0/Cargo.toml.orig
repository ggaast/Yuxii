[package]
name = "yuxii_macros"
version = "0.1.0"
edition = "2021"
author = "gaast"
license = "MIT OR Apache-2.0"
homepage = "https://github.com/ggaast/Yuxii/tree/main/crates/yuxii_macros"
repository = "https://github.com/ggaast/Yuxii/tree/main/crates/yuxii_macros"
